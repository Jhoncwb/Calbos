        <SQL_FORM DB="CIGAM_SQL" RESTAB="CIGAM_SQL">
          <SQL_STMT_U val="SELECT dbo.FAITEMPE.Documento&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;,dbo.FAITEMPE.Cd_material&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;,SUM(dbo.FAITEMPE.Quantidade) AS qntpedido&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;,ISNULL(and_disp_ret_saldo.total, 0) AS qntop&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;FROM dbo.FAITEMPE WITH (NOLOCK)&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;INNER JOIN fapedido WITH (NOLOCK) ON fapedido.Cd_pedido = dbo.FAITEMPE.Cd_pedido&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;LEFT JOIN and_disp_ret_saldo WITH (NOLOCK) ON and_disp_ret_saldo.op = dbo.FAITEMPE.Documento&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND and_disp_ret_saldo.cd_material = dbo.FAITEMPE.Cd_material&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE fapedido.Cd_pedido NOT BETWEEN '175557'&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND '176112'&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND fapedido.cd_tipo_operaca IN (&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;'6902A'&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;,'5902A'&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;,'6902M'&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;,'5902M'&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;)&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and FAPEDIDO.Cd_pedido = ':1'&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;GROUP BY dbo.FAITEMPE.Documento&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;,dbo.FAITEMPE.Cd_material&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;,and_disp_ret_saldo.total&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;HAVING ROUND(SUM(dbo.FAITEMPE.Quantidade), 3) > ROUND(ISNULL(and_disp_ret_saldo.total, 0), 3)&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;"/>
